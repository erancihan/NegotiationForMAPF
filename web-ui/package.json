{
  "name": "web-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "history": "^4.9.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "sockette": "^2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && yarn run move",
    "test": "react-scripts test",
    "move": "yarn run move-clean && yarn run move-index && yarn run move-public",
    "move-clean": "rm -rf -v ../agent/src/main/resources/public/*",
    "move-index": "mv -v build/index.html ../agent/src/main/resources/templates/index.html",
    "move-public": "mv -v build/* ../agent/src/main/resources/public/",
    "prettier-watch": "onchange '**/*.{js,css}' -- prettier --single-quote --write {{changed}}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "concurrently": "^4.1.0",
    "onchange": "^5.2.0",
    "prettier": "^1.17.1"
  }
}
